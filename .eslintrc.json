{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"parser": "babel-eslint",
	"extends": "eslint:recommended",
	"rules": {
		"indent": "off",
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"no-console": "warn",
		"no-async-promise-executor": "off",
		"no-ex-assign": "warn",
		"no-extra-parens": 1,
		"no-inner-declarations": "warn",
		"default-case": "error",
		"default-case-last": "warn",
		"no-unused-vars": "off"
	}
}
